{
  "name": "obrowser",
  "version": "0.0.4",
  "description": "Cross-platform Object Storage Browser desktop application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "test": "jest"
  },
  "build": {
    "appId": "com.zhuermu.obrowser.app",
    "productName": "OBrowser",
    "copyright": "Copyright Â© 2025",
    "mac": {
      "identity": "Your Developer ID Application: Your name (xxxxxx)",
      "category": "public.app-category.utilities",
      "icon": "obrowser.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "obrowser.ico",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "publisherName": "Zhuermu",
      "verifyUpdateCodeSignature": false,
      "compression": "normal"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "OBrowser",
      "uninstallDisplayName": "OBrowser",
      "installerIcon": "obrowser.ico",
      "uninstallerIcon": "obrowser.ico",
      "installerHeaderIcon": "obrowser.ico",
      "menuCategory": true,
      "displayLanguageSelector": true,
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "icon": "obrowser.png"
    },
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "**/*",
      "!**/node_modules/**/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/**/{test,__tests__,tests,powered-test,example,examples}/**",
      "!**/node_modules/**/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/node_modules/**/*.map",
      "!**/node_modules/**/LICENSE*",
      "!**/node_modules/**/license*",
      "!**/node_modules/**/.npmignore",
      "!**/node_modules/**/AUTHORS*",
      "!**/node_modules/**/CONTRIBUTORS*",
      "!**/node_modules/**/CHANGELOG*",
      "!**/node_modules/**/History.md",
      "!**/node_modules/**/HISTORY.md",
      "!**/node_modules/**/docs/**",
      "!**/node_modules/**/doc/**",
      "!**/node_modules/**/coverage/**",
      "!**/node_modules/**/.github/**",
      "!**/node_modules/**/.vscode/**"
    ],
    "extraResources": [
      {
        "from": "obrowser.ico",
        "to": "obrowser.ico"
      },
      {
        "from": "obrowser.png",
        "to": "obrowser.png"
      }
    ],
    "asar": true,
    "compression": "maximum",
    "afterSign": null,
    "afterPack": null,
    "afterAllArtifactBuild": null,
    "npmRebuild": false,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "forceCodeSigning": false,
    "publish": null
  },
  "author": "zhuermu",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.732.0",
    "@azure/storage-blob": "^12.14.0",
    "ali-oss": "^6.17.1",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "jest": "^29.0.0",
    "png2icons": "^2.0.1",
    "sharp": "^0.33.5"
  }
}
